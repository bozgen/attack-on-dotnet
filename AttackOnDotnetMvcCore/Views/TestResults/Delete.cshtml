@model AttackOnDotnetMvcCore.Models.TestResult

@{
    ViewData["Title"] = "Delete";
    ViewBag.PageName = "Delete Test Result";
    ViewBag.UpperPageName = "Test Results";
    ViewBag.UpperRoute = "/TestResults/";
}

<partial name="_Breadcrumbs" />

<div class="container">
    <h3 class="mt-5">Are you sure you want to delete this?</h3>
    <div>
        <h4 class="mt-3">TestResult</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.UserID)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.UserID)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.TestDate)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TestDate)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Result)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Result)
            </dd>
        </dl>

        <form class="p-4 px-2" asp-action="Delete">
            <input type="hidden" asp-for="ID" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index">Back to List</a>
        </form>
    </div>
</div>

